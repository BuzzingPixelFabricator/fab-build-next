{
    "assets": "public/assets",
    "source": "assetsSource",
    "libSync": [
        "assetsSource/fonts",
        "assetsSource/img",
        "assetsSource/lib"
    ],
    "cssOptions": {
        "preserveCustomProperties": false
    },
    "enableCss": true,
    "minifyCss": true,
    "parseMediaQueriesInFileNames": true,
    "mediaQueryUnit": "px",
    "compileCss": {
        "cssFiles": [],
        "mixinFiles": []
    },
    "enableJs": true,
    "minifyJs": true,
    "jsNamespace": "FAB",
    "jsBuild": [],
    "enableJSHint": true,
    "enableJSCS": true,
    "sourceMaps": true,
    "proxy": false,
    "watch": [],
    "watchUsesPolling": false,
    "critical": {
        "destination": "public/criticalCss"
    },
    "postErrorsTo": {
        "url": false,
        "projectName": false,
        "formMessageKey": "message",
        "formPostData": {}
    }
}
